        <record id="view_authorize_net_form" model="ir.ui.view">
            <field name="name">view.incomm.cred.form</field>
            <field name="model">incomm.cred.details</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Incomm Configuration" version="7.0">
                    <group>
                        <field name="test_production" required="True" on_change="onchange_test_production(test_production,context)"/>
                        <field name="server_url"/>
                    </group>
                </form>
            </field>
        </record>
         <record model="ir.ui.view" id="view_authorize_net_tree">
            <field name="name">view.incomm.cred.tree</field>
            <field name="model">incomm.cred.details</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Incomm Configuration">
                        <field name="server_url" required="True"/>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_incomm_cred_information">
            <field name="name">Incomm Configuration</field>
            <field name="res_model">incomm.cred.details</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
