        <record id="survey_form_inherit" model="ir.ui.view">
            <field name="name">survey.form.inherit</field>
            <field name="model">survey</field>
            <field name="inherit_id" ref="survey.survey_form" />
            <field name="groups_id" eval="[(6,0, [ref('cox_communication.group_internal_operations'), ref('cox_communication.group_support'),ref('cox_communication.group_retail_sales_associate'),ref('cox_communication.group_retail_sales_manager'),ref('cox_communication.group_warehouse_cox'),ref('cox_communication.group_third_party_reseller')])]"/>
            <field name="arch" type="xml">

                <xpath expr="//form" position="attributes">
                   <attribute name="edit">0</attribute>
               </xpath>
               <xpath expr="//form" position="attributes">
                   <attribute name="create">0</attribute>
               </xpath>
               <xpath expr="//button[@string='Edit Survey']" position="attributes">
                   <attribute name="invisible">1</attribute>
               </xpath>

                 </field>
        </record>
          <record id="survey_tree_inherit_cox" model="ir.ui.view">
            <field name="name">survey.tree.inherit.cox</field>
            <field name="model">survey</field>
            <field name="inherit_id" ref="survey.survey_tree" />
            <field name="groups_id" eval="[(6,0, [ref('cox_communication.group_internal_operations'), ref('cox_communication.group_support'),ref('cox_communication.group_retail_sales_associate'),ref('cox_communication.group_retail_sales_manager'),ref('cox_communication.group_warehouse_cox'),ref('cox_communication.group_third_party_reseller')])]"/>
            <field name="arch" type="xml">

               <xpath expr="//tree" position="attributes">
                   <attribute name="create">0</attribute>
               </xpath>
                 </field>
        </record>
         <record id="survey_kanban_inherit_cox" model="ir.ui.view">
            <field name="name">survey.kanban.inherit.cox</field>
            <field name="model">survey</field>
            <field name="inherit_id" ref="survey.view_survey_kanban" />
            <field name="groups_id" eval="[(6,0, [ref('cox_communication.group_internal_operations'), ref('cox_communication.group_support'),ref('cox_communication.group_retail_sales_associate'),ref('cox_communication.group_retail_sales_manager'),ref('cox_communication.group_warehouse_cox'),ref('cox_communication.group_third_party_reseller')])]"/>
            <field name="arch" type="xml">

               <xpath expr="//kanban" position="attributes">
                   <attribute name="create">0</attribute>
               </xpath>
               <xpath expr="//kanban" position="attributes">
                   <attribute name="edit">0</attribute>
               </xpath>
                 </field>
        </record>
