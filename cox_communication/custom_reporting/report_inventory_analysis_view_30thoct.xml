        <record id="action_inventory_analysis_custom_report" model="ir.actions.act_window">
            <field name="name">Inventory Analysis(Custom)</field>
            <field name="res_model">report.inventory.analysis.custom</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="domain">['|',('active','=',True),('active','=',False)]</field>
<!--            <field name="search_view_id"  ref="view_report_inventory_analysis_custom_search"/> -->
