        <record id="view_report_inventory_analysis_custom_tree" model="ir.ui.view">
            <field name="name">report.inventory.analysis.custom.tree</field>
            <field name="model">report.inventory.analysis.custom</field>
            <field name="arch" type="xml">
                <tree string="Moves Analysis" create="false">
                    <field name="location_id" invisible="1"/>
                    <field name="active"/>
                    <field name="product_id" invisible="1"/>
                    <field name="total_product_qty" sum="Total quantity"/>
                    <field name="current_product_qty" sum="Total value"/>
                    <field name="previous_product_qty" sum="Total value"/>
                    <field name="yesterday_so_qty" sum="Total value "/>
                    <field name="yesterday_exchng_qty" sum="Total value "/>
                </tree>
            </field>
        </record>

