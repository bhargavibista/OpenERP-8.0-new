                        <field name="date_to"/>
                    </group>
                    <field name="name" readonly="1" invisible="1" colspan="2" attrs="{'invisible':[('csv_file','=','')]}"/>
                    <newline/>
                    <button special="cancel" string="Cancel" icon="gtk-cancel"/>
                    <button name="export_cancel_service_csv" string="Export" type="object" icon="gtk-apply"/>
                </form>
            </field>
        </record>
        <!--Cancel Services reference creation-->

