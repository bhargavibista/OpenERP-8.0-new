<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_export_csv_wizard" model="ir.ui.view">
            <field name="name">Export CSV</field>
            <field name="model">export.csv</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Export CSV">
                    <separator string="Sales Report" colspan="4"/>
                    <group colspan="2">
                        <field name="csv_file" string="File"  readonly="1" filename='name'  attrs="{'invisible':[('name','=','')]}"/>
						<field name="date_from"/>
                        <field name="date_to"/>
                    </group>
                    <field name="name" readonly="1" invisible="1" colspan="2" attrs="{'invisible':[('csv_file','=','')]}"/>
                    <newline/>
                    <button special="cancel" string="Cancel" icon="gtk-cancel"/>
                    <button name="export_csv" string="Export" type="object" icon="gtk-apply"/>
                </form>
            </field>
        </record>

        <!--Cancel Services reference creation-->
        <record id="view_export_cancel_servicecsv_wizard" model="ir.ui.view">
