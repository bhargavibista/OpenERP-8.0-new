<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data>
        <record id="view_pre_picking_scanning" model="ir.ui.view">
            <field name="name">view.pre.picking.scanning</field>
            <field name="model">pre.picking.scanning</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Pre Picking and Scanning">
                    <!--<separator string="" colspan="8"/>-->
                    <group>
                        <group>
                            <button name="barcode_scanning" string="BarCode Scanning" type="object"/>
                        </group>
                        <group>
                            <button name="import_serial_number" string="Scan Multiple Quantity" type="object"
                                    invisible="'trigger' in context"/>
                        </group>
                    </group>
                    <button name="default_picking_flow" string="Skip BarCode Scanning" type="object" invisible="1"/>

                </form>
            </field>
        </record>
    </data>
</openerp>
