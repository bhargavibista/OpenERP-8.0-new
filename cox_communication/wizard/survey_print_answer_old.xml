<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_cox_print_survey_answers" model="ir.ui.view">
            <field name="name">view.cox.print.survey.answers</field>
            <field name="model">survey.print.answer</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="survey.view_survey_print_answer"/>
            <field name="arch" type="xml">
                <xpath expr="/form/header/button[@name='action_next']" position="attributes">
                    <attribute name="string">Print in PDF</attribute>
                </xpath>
                <xpath expr="/form/header/button[@name='action_next']" position="after">
                    <button name="print_anwser_excel" string="Print in Excel" type="object" class="oe_highlight"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>