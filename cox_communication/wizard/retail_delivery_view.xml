<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_retail_delivery" model="ir.ui.view">
            <field name="name">view.retail.delivery</field>
            <field name="model">retail.delivery</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Retail Delivery">
                    <field name="stock_available" invisible="True"/>
                    <field name="so_line_ids" invisible="True"/>
                    <button name="barcode_scanning" string="In Store to Customer" type="object" attrs="{'invisible':[('stock_available','=',False)]}"/>
                    <button name="delivery_later" string="Ship to Customer" type="object"/>
                </form>
            </field>
        </record>
    </data>
</openerp>